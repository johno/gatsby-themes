{
  "name": "gatsby-ui",
  "version": "0.0.2",
  "description": "Design system and component library for Gatsby themes",
  "license": "MIT",
  "keywords": [
    "process",
    "design",
    "system",
    "gatsby"
  ],
  "repository": "johno/gatsby-themes",
  "author": "John Otander <johnotander@gmail.com> (http://johnotander.com)",
  "contributors": [
    "John Otander <johnotander@gmail.com> (http://johnotander.com)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@mdx-js/tag": "^0.16.6",
    "prism-react-renderer": "^0.1.5",
    "prop-types": "^15.6.2",
    "styled-components": "^4.1.3",
    "styled-system": "^3.1.11"
  },
  "devDependencies": {
    "@compositor/kit": "^1.0.47",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.15.3",
    "remark-cli": "^6.0.1",
    "remark-preset-wooorm": "^4.0.0",
    "xo": "^0.23.0"
  },
  "scripts": {
    "build": "babel src -d dist",
    "format": "remark . -qfo && prettier \"**/*.js\" --write && xo --fix",
    "now-build": "x0 build docs",
    "prepare": "rm -rf dist && npm run build",
    "start": "x0 docs",
    "test": "npm run format && npm run build"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "extends": [
      "xo-react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "unicorn/filename-case": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
