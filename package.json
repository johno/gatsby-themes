{
  "private": true,
  "name": "gatsby-themes",
  "repository": "johno/gatsby-themes",
  "license": "MIT",
  "scripts": {
    "start": "yarn workspace gatsby-starter-blog-theme start",
    "prepare": "lerna bootstrap",
    "publish": "lerna publish",
    "publish:ci": "lerna publish -y --canary --preid ci --npm-tag=ci --force-publish=*",
    "cli": "yarn workspace gatsby-starter-blog-theme ui"
  },
  "author": "John Otander <johnotander@gmail.com> (http://johnotander.com)",
  "workspaces": [
    "themes/*",
    "examples/*",
    "packages/*"
  ],
  "dependencies": {
    "lerna": "^3.16.4"
  }
}
